{
  "name": "xzy-materials",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dumi dev",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "release": "changeset publish",
    "version-packages": "changeset version",
    "version-and-tag": "changeset version && git add .changeset package.json packages/*/package.json && git commit -m 'Update versions' && git tag -m 'Release versions' v$(node -e \"console.log(require('./package.json').version)\")",
    "changeset": "changeset",
    "deploy": "gh-pages -d docs-dist",
    "docs-build-and-publish": "dumi build && gh-pages -d docs-dist"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.27.8",
    "@changesets/git": "^3.0.4",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "antd": "^5.20.0",
    "dumi": "^2.4.14",
    "gh-pages": "^6.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.3.5",
    "turbo": "^2.1.3",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.0"
}
